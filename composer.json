{
    "name": "wangaz/laravel-semantic-ui-css",
    "description": "Semantic UI Implementation for Laravel",
    "keywords": ["css", "laravel", "semantic ui"],
    "license": "LGPL v3",
    "version": "0.2.1",
    "type": "laravel-package",
    "require": {
        "laravel/framework": "5.2.*"
    },
    "provide": {
	    "semantic/ui-css": "2.1.7"
    },
    "autoload": {
        "psr-4": {
            "SUI\\": "src/"
        }
    },
    "scripts": {
      "post-update-cmd": "php artisan vendor:publish --provider=\"SUI\\SUIServiceProvider\" --tag=public --force"
    }
}
